{
  "name": "markdown-search",
  "displayName": "Markdown Full Text Search",
  "description": "Markdown Full Text Search makes it easy to search for text in Markdown files. With this extension, you can quickly find the text you're looking for and link it.",
  "publisher": "AlexandriteSoftware",
  "preview": true,
  "version": "0.1.8",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexandriteSoftware/markdown-search.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "markdown-search.search",
        "title": "Markdown Search: Search in Markdown files"
      },
      {
        "command": "markdown-search.add-or-replace-link",
        "title": "Markdown Search: Add link from search results"
      }
    ],
    "configuration": {
      "title": "Markdown Full Text Search",
      "properties": {
        "markdown-search.logging.level": {
          "type": "string",
          "default": "info",
          "enum": [
            "debug",
            "info",
            "notice",
            "warning",
            "error",
            "crit",
            "alert",
            "emerg",
            "none"
          ],
          "scope": "window",
          "description": " none, or RFC5424 logging level: debug, info, notice, warning, error, crit, alert, emerg. Requires restart."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "esbuild": "^0.18.13",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "minimatch": "^3.1.2",
    "minisearch": "^6.1.0",
    "winston": "^3.10.0"
  }
}
